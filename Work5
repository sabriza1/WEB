const readline = require("readline");

const rl = readline.createInterface({
  input: process.stdin,
  output: process.stdout
});

rl.question("–ö–∞–∫–∞—è —Å–µ–π—á–∞—Å —Ç–µ–º–ø–µ—Ä–∞—Ç—É—Ä–∞ –Ω–∞ —É–ª–∏—Ü–µ? ", function(temp) {
  temp = Number(temp);

  if (temp <= 0) {
    console.log("‚ùÑÔ∏è –û—á–µ–Ω—å —Ö–æ–ª–æ–¥–Ω–æ! –ù–∞–¥–µ–Ω—å —Ç—ë–ø–ª—É—é –∫—É—Ä—Ç–∫—É, —à–∞–ø–∫—É –∏ –ø–µ—Ä—á–∞—Ç–∫–∏.");
  } else if (temp > 0 && temp <= 10) {
    console.log("ü•∂ –ü—Ä–æ—Ö–ª–∞–¥–Ω–æ. –õ—É—á—à–µ –Ω–∞–¥–µ—Ç—å –∫—É—Ä—Ç–∫—É –∏ —à–∞—Ä—Ñ.");
  } else if (temp > 10 && temp <= 20) {
    console.log("üå§Ô∏è –ù–µ–º–Ω–æ–≥–æ –ø—Ä–æ—Ö–ª–∞–¥–Ω–æ, –Ω–∞–¥–µ–Ω—å –ª—ë–≥–∫—É—é –∫—É—Ä—Ç–∫—É –∏–ª–∏ —Ç–æ–ª—Å—Ç–æ–≤–∫—É.");
  } else if (temp > 20 && temp <= 30) {
    console.log("‚òÄÔ∏è –¢–µ–ø–ª–æ! –ú–æ–∂–Ω–æ –Ω–∞–¥–µ—Ç—å —Ñ—É—Ç–±–æ–ª–∫—É –∏ –¥–∂–∏–Ω—Å—ã.");
  } else {
    console.log("üî• –û—á–µ–Ω—å –∂–∞—Ä–∫–æ! –ù–∞–¥–µ–Ω—å –ª—ë–≥–∫—É—é –æ–¥–µ–∂–¥—É –∏ –Ω–µ –∑–∞–±—É–¥—å –≤–æ–¥—É.");
  }

  rl.close();
});
