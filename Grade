score=int(input("Оценка (0-100):"))
if score >=90:
    hw = input("Сдал все дз? (да/нет):")
    if hw == "да":
        print("Отлично! A+") 
    else:
        print( "Хорошая работа, но сдайте все задания! A")
elif 80 <=score <=89:
  att=input("Посещаемость>75%?(да/нет):")
  if att == "да":
     print("Хорошо! B") 
  else:
    print( "Нужно посещать занятия! C")
else:
  print("Старайтесь лучше!")
