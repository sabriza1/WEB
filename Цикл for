//1
print("Все числа от 1 до 20:")
for i in range(1, 21):
    print(i, end=" ")
print("\n")  # перенос строки

//2
print("Чётные числа от 1 до 20:")
for i in range(1, 21):
    if i % 2 == 0:
        print(i, end=" ")
print("\n")

//3
sum_odd = 0
for i in range(1, 21):
    if i % 2 != 0:
        sum_odd += i

print("Сумма нечётных чисел от 1 до 20:", sum_odd)
